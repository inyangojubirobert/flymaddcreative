{
  "version": 2,
  "framework": "nextjs",
  "buildCommand": "npm run build",
  "outputDirectory": ".next",
  "installCommand": "npm install",
  "trailingSlash": false,
  "cleanUrls": true,
  "rewrites": [
    { "source": "/", "destination": "/index.html" },
    { "source": "/websiteapp", "destination": "/websiteapp.html" },
    { "source": "/Onedream", "destination": "/websiteapp.html" }
  ],
  "headers": []
}
