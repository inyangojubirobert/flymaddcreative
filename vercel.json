{
  "version": 2,
  "buildCommand": "echo 'Static site - no build needed'",
  "outputDirectory": ".",
  "installCommand": "echo 'No dependencies to install'",
  "framework": null,
  "public": true,
  "trailingSlash": false,
  "cleanUrls": true,
  "rewrites": [
    {
      "source": "/",
      "destination": "/websiteapp.html"
    },
    {
      "source": "/app",
      "destination": "/websiteapp.html"
    },
    {
      "source": "/onedream",
      "destination": "/Onedream.html"
    }
  ],
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "X-Content-Type-Options",
          "value": "nosniff"
        },
        {
          "key": "X-Frame-Options",
          "value": "DENY"
        },
        {
          "key": "X-XSS-Protection",
          "value": "1; mode=block"
        }
      ]
    },
    {
      "source": "/Onedream.html",
      "headers": [
        {
          "key": "X-Frame-Options",
          "value": "SAMEORIGIN"
        }
      ]
    }
  ]
}
